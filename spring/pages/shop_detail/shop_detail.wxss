page {
    background-color: rgb(240, 240, 240);
    height: 100%;
    width: 100%;
}

@font-face {
    font-family: "iconfont";
    src: url('iconfont.eot?t=1565608797771');
    /* IE9 */
    src: url('iconfont.eot?t=1565608797771#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'), url('iconfont.woff?t=1565608797771') format('woff'), url('iconfont.ttf?t=1565608797771') format('truetype'), /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+ */
    url('iconfont.svg?t=1565608797771#iconfont') format('svg');
    /* iOS 4.1- */
}

button::after {
    border: none;
    outline: none;
}

button {
    background-color: rgba(0, 0, 0, 0);
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 50rpx;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-zhuye:before {
    content: "\e604";
}

.icon-fenxiang:before {
    content: "\e627";
}

swiper {
    height: 300rpx; 
    background-color: white;
}

.body_top image {
    width: 100%;
}

.user_message {
    display: flex;
    flex-direction: row;
    background-color: white;
    padding: 10rpx;
}

.user_image image {
    width: 120rpx;
    border-radius: 50%;
}

.user_message_detail {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 10rpx;
}

.user_message_detail_title {
    font-weight: 800;
    color: cadetblue;
}

.user_message_detail_place {
    font-weight: 200;
    font-size: 25rpx;
}

.user_message_detail_place text {
    color: tan;
}

.user_message_detail_intro {
    background-color: white;
    padding: 10rpx;
    padding-top: 0rpx;
    font-size: 27rpx;
    letter-spacing: 5rpx;
}

.main_goods {
    background-color: white;
    margin-top: 10rpx;
    padding: 10rpx;
}

.main_goods_title {
    font-weight: 800;
}

.main_goods_item {
    margin-top: 10rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    flex-wrap: wrap;
}

.main_good {
    display: flex;
    flex-direction: column;
    margin: 10rpx;
}

.main_goods_item_image image {
    width: 340rpx;
}

.main_goods_item_title {
    font-size: 30rpx;
}

.price {
    color: red;
    font-size: 25rpx;
}

.map {
    background-color: white;
    padding: 20rpx;
    margin-top: 10rpx;
}

.map_back map {
    margin-top: 10rpx;
    width: 97%;
    padding: 10rpx;
    height: 300rpx;
}

.tips {
    padding: 10rpx;
    height: 200rpx;
    color: red;
    font-size: 25rpx;
}

.bottom {
    position: fixed;
    bottom: 0;
    width: 100%;
    background-color: white;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    z-index: 999999;
    height: 80rpx;
    padding: 10rpx;
}

.other_operation {
    display: flex;
    flex-direction: row;
}

.zhuye {
    margin-right: 50rpx;
}

.share {
    margin-left: 50rpx;
}

.phone {
    font-weight: 800;
    font-size: 32rpx;
    color: coral;
}